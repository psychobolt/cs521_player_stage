// Set configurations for all format
* ALL:
    FORMAT			=	"%time %level %log"
    FILENAME			=       "/tmp/log/easylogging++-config-builder.log"
    ENABLED			=	true
    TO_FILE			=	true
    TO_STANDARD_OUTPUT		=	true
    MILLISECONDS_WIDTH		=	3
    PERFORMANCE_TRACKING	=	false
    ROLL_OUT_SIZE               =       52428800 // Throw log file away every 50 MB
* DEBUG:

    FILENAME			=       "/tmp/log/easylogging++-config-builder-debug.log"
// Trace logs
* TRACE: 
    FORMAT			=	"%time %level [%loc] %log"
// Verbose logs
* VERBOSE:
    FORMAT                      =       "%time %level-%vlevel %log"
